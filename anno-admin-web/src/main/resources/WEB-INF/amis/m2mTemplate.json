{
  "title": "",
  "subTitle": "",
  "type": "page",
  "body": {
    "type": "crud",
    "draggable": false,
    "perPage": 10,
    "id": "m2m-crud",
    "syncLocation": false,
    "api": {
      "method": "post",
      "url": "/system/anno/${clazz}/page",
      "data": {
        "&": "$$",
        "_cat": "${_cat}",
        "_extraData": "${extraData}"
      }
    },
    "onEvent": {
      "broadcast_refresh_m2m_crud": {
        "actions": [
          {
            "componentId": "m2m-crud",
            "actionType": "reload"
          }
        ]
      }
    },
    "keepItemSelectionOnPageChange": true,
    "headerToolbar": [
      "bulkActions",
      "reload",
      {
        "type": "button",
        "label": "新增",
        "level": "primary",
        "actionType": "dialog",
        "dialog": {
          "title": "新增",
          "size": "full",
          "body":[ {
            "type": "select",
            "name": "selectTable",
            "selectMode": "table",
            "multiple": true,
            "columns": [

            ],
            "source": "/system/anno/${clazz}/m2mSelect?_extraData=${extraData}"
          }],
          "onEvent": {
            "confirm": {
              "actions": [
                {
                  "args": {
                    "options": {},
                    "api": {
                      "url": "/system/anno/${clazz}/addM2m",
                      "method": "post",
                      "data": {
                        "&": "$$",
                        "_extraData": "${extraData}"
                      }
                    }
                  },
                  "outputVar": "responseResult",
                  "actionType": "ajax"
                },{
                  "actionType": "broadcast",
                  "args": {
                    "eventName": "broadcast_refresh_m2m_crud"
                  },
                  "data": {
                  }
                }
              ]
            }
          }
        }
      }
    ],
    "footerToolbar": [
      "statistics",
      "switch-per-page",
      "pagination"
    ],
    "columns": [
      {
        "type": "operation",
        "label": "操作",
        "buttons": [
        ],
        "fixed": "right"
      }
    ]
  },
  "asideResizor": true,
  "pullRefresh": {
    "disabled": true
  },
  "asideMinWidth": 170,
  "asideMaxWidth": 350
}