{
  "title": "",
  "subTitle": "",
  "type": "page",
  "body": {
    "type": "crud",
    "draggable": false,
    "perPage": 10,
    "id": "m2m-crud",
    "syncLocation": false,
    "api": {
      "method": "post",
      "url": "/system/anno/${clazz}/page",
      "data": {
        "&": "$$",
        "_cat": "${_cat}",
        "_extraData": "${extraData}"
      }
    },
    "onEvent": {
      "broadcast_refresh_m2m_crud": {
        "actions": [
          {
            "componentId": "m2m-crud",
            "actionType": "reload"
          }
        ]
      }
    },
    "keepItemSelectionOnPageChange": true,
    "headerToolbar": [
      "bulkActions",
      "reload",
      {
        "type": "button",
        "label": "新增关联关系",
        "level": "primary",
        "actionType": "dialog",
        "dialog": {
          "title": "新增关联关系",
          "size": "full",
          "body":[],
          "actions": [{
            "type": "action",
            "actionType": "confirm",
            "label": "操作完成",
            "size": "lg",
            "level": "primary"
          }],
          "showCloseButton": false,
          "onEvent": {
            "confirm": {
              "actions": [
                {
                  "actionType": "broadcast",
                  "args": {
                    "eventName": "broadcast_refresh_m2m_crud"
                  },
                  "data": {
                  }
                }
              ]
            }
          }
        }
      }
    ],
    "footerToolbar": [
      "statistics",
      "switch-per-page",
      "pagination"
    ],
    "columns": [
      {
        "type": "operation",
        "label": "操作",
        "buttons": [
        ],
        "fixed": "right"
      }
    ]
  },
  "asideResizor": true,
  "pullRefresh": {
    "disabled": true
  },
  "asideMinWidth": 170,
  "asideMaxWidth": 350
}